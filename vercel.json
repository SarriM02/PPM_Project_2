{
  "builds": [{
    "src": "PPM_Project/wsgi.py",
    "use": "@vercel/python",
    "config": { "maxLambdaSize": "15mb" , "runtime": "python3.8" }
  }],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "s-maxage=31536000,immutable" },
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "PPM_Project/wsgi.py"
    }
  ]
}